#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
    cin >> n;
    // n=2
    for(int i=0;i<n;i++){// i=0
        string s,s1,s2;
            cin >> s; // Hacker
         for (int j=0;j<s.length();j++){ //j=0, length=6     
                if(j%2==0){
                s1 +=s[j];
                }
                else {
                s2 +=s[j];
                }
         }
        cout<<s1<<" "<<s2<<"\n";
    }
      
    return 0;
}

